/* 
  Name:			Roll Out
  Type:			Exit
  Description:	Roll out Effect
  Version:		2.0	
  locale:		{"zh-Hans":"卷出", "en-us":"Roll Out"}
 */
 
@keyframes rollOut {
  0% {
    opacity: 1;
  }

  100% {
    opacity: 0;
    transform: translate3d(100%, 0, 0) rotate3d(0, 0, 1, 120deg);
  }
}

.rollOut {
  animation-name: rollOut;
}